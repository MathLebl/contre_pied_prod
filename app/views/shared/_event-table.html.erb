  <tbody>
    <% @events.each do |event|%>
    <tr>
      <td class="table-date"><%= l event.date %></td>
      <td class="table-name">
        <%= link_to artist_path(event.artist_id) do %>
          <%= event.artist.name %>
        <% end %>
      </td>
      <td class="table-location"><%= event.location %></td>
      <% if event.ticket != nil %>
        <td class="table-ticket"><a href="<%= event.ticket %>" target="_blank"><i class="fas fa-ticket-alt"></i></a></td>
      <% else %>
        <td></td>
      <% end %>
    </tr>
    <% end %>
  </tbody>
