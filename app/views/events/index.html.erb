<div class="container">
  <h1>Agenda</h1>
  <p>Retrouvez nos spectacles, partout en France</p>
  <table class="table table-hover table-primary">
    <tbody>
      <% @events.each do |event|%>
      <tr>
        <td><%= event.date %></td>
        <td><%= event.artist.name %></td>
        <td><%= event.location %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
